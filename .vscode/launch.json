{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Node process",
      "processId": "${command:PickProcess}",
      "skipFiles": ["<node_internals>/**"],
      "restart": true,
      "sourceMaps": true,
      "trace": true,
      "outFiles": ["${workspaceFolder}/.next/**/*.js"]
    }
  ]
}
